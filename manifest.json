{
  "name": "Symfony Doc Force Version",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Force Symfony Doc version",
  "homepage_url": "https://github.com/ByScripts/symfony-doc-force-version",
  "author" : "Thierry Goettelmann <thierry@byscripts.info>",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Symfony Doc Force Version"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://symfony.com/*"
  ]
}
